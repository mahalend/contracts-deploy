{
  "name": "@mahalend/deploy",
  "version": "2.6.0",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "contracts",
    "artifacts",
    "types"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "types": "dist/src/helpers/index.d.ts",
  "main": "dist/src/helpers/index.js",
  "devDependencies": {
    "@mahalend/core": "^2.0.0",
    "@mahalend/periphery": "^3.3.1",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.5",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/buidler": "^1.4.8",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.3.2",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/bluebird": "^3.5.36",
    "@types/chai": "^4.2.21",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.1",
    "bluebird": "^3.7.2",
    "chai": "^4.3.4",
    "chalk": "^4.1.2",
    "defender-relay-client": "^1.11.1",
    "dotenv": "^10.0.0",
    "eth-sig-util": "^3.0.1",
    "ethereumjs-util": "^7.1.5",
    "ethers": "^5.4.7",
    "hardhat": "^2.12.4",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-dependency-compiler": "^1.1.2",
    "hardhat-deploy": "^0.11.22",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-typechain": "^0.3.5",
    "jsondiffpatch": "^0.4.1",
    "prettier": "^2.3.2",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typechain": "^8.1.1",
    "typescript": "^4.3.2"
  },
  "license": "AGPLv3",
  "scripts": {
    "run-env": "npm ci && tail -f /dev/null",
    "cp-typechain": "cp typechain/*.ts ./dist/types/typechain",
    "build": "tsc --project lib.tsconfig.json && npm run cp-typechain",
    "clean": "rm -rf artifacts/ types/ dist/ typechain/",
    "compile": "SKIP_LOAD=true npx hardhat compile",
    "prepublish": "npm run clean && npm run compile && npm run build",
    "node": "npx hardhat node --no-deploy",
    "node:fork": "FORK=main npm run node --no-deploy",
    "deploy": "if [ \"$SKIP_COMPILE\" = 'true' ]; then echo '[Warning] Skipping compilation'; else npm run compile; fi && npx hardhat deploy --no-compile",
    "test": "npm run compile && npm run test:local && npm run test:fork",
    "test:fork": "MARKET_NAME=Aave FORK=main npx hardhat test ./tests/__setup.spec.ts ./tests/fork/check-list.spec.ts",
    "test:local": "MARKET_NAME=Aave npx hardhat test ./tests/__setup.spec.ts ./tests/*.spec.ts",
    "test:ethereum:fork": "TRANSFER_OWNERSHIP=true  FORK=main MARKET_NAME=Ethereum npx hardhat test ./tests/ethereum/ethereum.*.spec.ts",
    "deploy:market:aave:fork": "FORK=main MARKET_NAME=Aave npm run deploy",
    "deploy:market:eth:empty:fork": "FORK=main MARKET_NAME=Ethereum npm run deploy"
  },
  "peerDependencies": {
    "@mahalend/core": "x || >=1.0.0-beta || >=3.0.0-beta",
    "@mahalend/periphery": "x || >=1.0.0-beta || >=3.0.0-beta",
    "hardhat": "^2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mahalend/contracts-deploy.git"
  }
}
